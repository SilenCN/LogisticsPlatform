<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.UserDao">
    <insert id="createUser" useGeneratedKeys="true" keyProperty="id" parameterType="com.model.User">
        INSERT INTO User (name,password,type,createTime) VALUES (#{name},#{password},#{type},#{createTime})
    </insert>
    <select id="selectAll" resultType="com.model.User">
        select * from User
    </select>

</mapper>